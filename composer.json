{
    "name": "web-id-fr/matryoshka",
    "description": "Russian Doll Caching for Laravel.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "cache"
    ],
    "authors": [
        {
            "name": "Jeffrey Way",
            "email": "jeffrey@laracasts.com"
        },
        {
            "name": "Wilfried Jourdil",
            "email": "wilfried@web-id.fr"
        }
    ],
    "require": {
        "illuminate/support": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Laracasts\\Matryoshka\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "illuminate/database": "^5.1",
        "illuminate/cache": "^5.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laracasts\\Matryoshka\\MatryoshkaServiceProvider"
            ]
        }
    }
}
